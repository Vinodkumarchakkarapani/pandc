#Declaring objects with css and xpath locators
@objects
  Breadcrumbs                        css    .breadcrumb-tab
  Header                             css    #main-id .new-admin-header
  TabBreadcrumbs                     css    #main-id .container-fluid .breadcrumb-tab
  InfoHeader                         css    #main-id .name-insured
  Label-*                            css    #myTabContent label
  Input-*                            css    #myTabContent input
  Select-*                           css    #myTabContent select
  TextArea-*                         css    #myTabContent textarea
  H4Text-*                           css    #myTabContent h4
  H2Text-*                           css    #myTabContent h2
  Alert-*                            css    #myTabContent .spnAlert
  Anchor-*                           css    #myTabContent a
  Button-*                           css    #myTabContent button
  Span-*                             css    #main-id span


#RFRCover Page
= RFRCover page =
  #This is for Desktop
   @on desktop

       #Breadcrumbs properties
        Breadcrumbs:
           css display is "block"
           css color contains "33, 37, 41"
           css font-size is "16px"

       #Header properties
        Header:
           css display is "block"
           css color contains "33, 37, 41"
           css font-size is "16px"

       #TabBreadcrumbs properties
        TabBreadcrumbs:
           css display is "block"
           css color contains "33, 37, 41"
           css font-size is "16px"

       #InfoHeader properties
        InfoHeader:
           css display is "flex"
           css color contains "33, 37, 41"
           css font-size is "16px"

       #Label properties
        @for [47] as index
           Label-${index}:
              css display is "block"
              css color contains "33, 37, 41"
              css font-size is "16px"

       #Label properties
        @for [48] as index
           Label-${index}:
              css display is "inline-block"
              css color contains "255, 255, 255"
              css font-size is "14px"

       #Label properties
        @for [49, 50 - 61 ] as index
           Label-${index}:
              css display is "inline-block"
              css color contains "31, 42, 68"
              css font-size is "14px"

       #Input properties
        @for [127 - 128, 130 - 131, 133 - 134, 136 - 137, 139 - 140, 142 - 143, 145 - 146, 148 - 149, 151 - 152, 154 - 155, 157 -158, 160 -161, 163 - 164, ] as index
           Input-${index}:
              css display is "inline-block"
              css color contains "91, 103, 112"
              css font-size is "14px"

       #Button properties
        @for [40-43] as index
           Button-${index}:
              css display is "block"
              css color contains "255, 255, 255"
              css font-size is "12px"

       #TextArea properties
        @for [6] as index
           TextArea-${index}:
              css display is "block"
              css color contains "91, 103, 112"
              css font-size is "14px"

       #H2Text properties
        @for [4] as index
           H2Text-${index}:
              css display is "block"
              css color contains "31, 42, 68"
              css font-size is "18px"

       #H2Text properties
        @for [5 - 7] as index
           H2Text-${index}:
              css display is "block"
              css color contains "255, 255, 255"
              css font-size is "14px"

       #Anchor properties
        @for [10, 12 - 13] as index
           Anchor-${index}:
              css display is "block"
              css color contains "0, 123, 255"
              css font-size is "16px"

       #Span properties
        @for [1] as index
           Span-${index}:
              css display is "block"
              css color contains "91, 103, 112"
              css font-size is "18px"

       #Span properties
        @for [3] as index
           Span-${index}:
              css display is "inline"
              css color contains "153, 153, 153"
              css font-size is "14px"

       #Span properties
        @for [6, 8, 10, 12, 14, 16, 18] as index
           Span-${index}:
              css display is "block"
              css color contains "255, 255, 255"
              css font-size is "16px"

       #Span properties
        @for [7, 9, 11, 13, 15, 17, 19] as index
           Span-${index}:
              css display is "inline"
              css color contains "255, 255, 255"
              css font-size is "11px"

       #Span properties
        @for [20 - 21] as index
           Span-${index}:
              css display is "inline"
              css color contains "31, 42, 68"
              css font-size is "16px"

      #Span properties
        @for [57] as index
           Span-${index}:
              css display is "inline-block"
              css color contains "91, 103, 112"
              css font-size is "12px"

       #Span properties
        @for [56] as index
           Span-${index}:
              css display is "block"
              css color contains "91, 103, 112"
              css font-size is "14px"

      #Span properties
        @for [58] as index
           Span-${index}:
              css display is "inline-block"
              css color contains "31, 42, 68"
              css font-size is "14px"

      #Span properties
        @for [59] as index
           Span-${index}:
              css display is "inline-block"
              css color contains "91, 103, 112"
              css font-size is "14px"