#Declaring objects with css and xpath locators
@objects
  Breadcrumbs                        css    .breadcrumb-tab
  Header                             css    #main-id .new-admin-header
  TabBreadcrumbs                     css    #main-id .container-fluid .breadcrumb-tab
  InfoHeader                         css    #main-id .name-insured
  Label-*                            css    #main-id label
  Input-*                            css    #main-id input
  Select-*                           css    #main-id select
  TextArea-*                         css    #main-id textarea
  Anchor-*                           css    #main-id a
  Button-*                           css    #main-id button
  Table-*                            css    #exposure-table

#RFRCasualtyExporsureRevenueLiabilityLimits Page
= RFRCasualtyExporsureRevenueLiabilityLimits page =
  #This is for Desktop
     @on desktop

         #Breadcrumbs properties
          Breadcrumbs:
            css display is "block"
            css color contains "33, 37, 41"
            css font-size is "16px"

         #Header properties
          Header:
            css display is "block"
            css color contains "33, 37, 41"
            css font-size is "16px"

         #TabBreadcrumbs properties
          TabBreadcrumbs:
            css display is "block"
            css color contains "33, 37, 41"
            css font-size is "16px"

         #InfoHeader properties
          InfoHeader:
            css display is "flex"
            css color contains "33, 37, 41"
            css font-size is "16px"

         #Label properties
          Label-1:
             css display is "block"
             css color contains "33, 37, 41"
             css font-size is "16px"

         #Label properties
          @for [2, 6] as index
            Label-${index}:
              css display is "inline-block"
              css color contains "255, 255, 255"
              css font-size is "14px"

         #Label properties
          @for [3 - 5, 7 - 20] as index
            Label-${index}:
              css display is "inline-block"
              css color contains "91, 103, 112"
              css font-size is "14px"

         #Input properties
          @for [5 - 8, 10 - 13, 15 - 18, 21 - 22, 24 -25, 27 -28, 30 - 31, 33 - 34, 36 - 37, 39 - 40, 42 - 43, 45 - 46, 48 - 49, 51 - 52, 54 - 55, 57 - 58, 60 - 61] as index
            Input-${index}:
              css display is "inline-block"
              css color contains "91, 103, 112"
              css font-size is "14px"

         #TextArea properties
          TextArea-1:
            css display is "block"
            css color contains "91, 103, 112"
            css font-size is "14px"

         #Anchor properties
          @for [1 , 3 ] as index
            Anchor-${index}:
              css display is "block"
              css color contains "0, 123, 255"
              css font-size is "16px"

         #Anchor properties
          Anchor-6:
            css display is "block"
            css color contains "120, 190, 32"
            css font-size is "14px"

         #Anchor properties
          Anchor-7:
            css display is "block"
            css color contains "31, 42, 68"
            css font-size is "14px"

         #Anchor properties
          @for [15 - 16] as index
             Anchor-${index}:
               css display is "block"
               css color contains "0, 115, 150"
               css font-size is "14px"

         #Anchor properties
          @for [17 - 23] as index
             Anchor-${index}:
                css display is "block"
                css color contains "255, 255, 255"
                css font-size is "16px"

         #Anchor properties
          @for [25 - 30] as index
             Anchor-${index}:
                css display is "block"
                css color contains "255, 255, 255"
                css font-size is "14px"

         #Anchor properties
          Anchor-24:
             css display is "block"
             css color contains "73, 80, 87"
             css font-size is "14px"

         #Anchor properties
          Anchor-31:
             css display is "block"
             css color contains "255, 255, 255"
             css font-size is "12px"

         #Anchor properties
          @for [32 - 33] as index
             Anchor-${index}:
                css display is "block"
                css color contains "0, 123, 255"
                css font-size is "16px"

         #Button properties
          @for [1, 8, 9, 76, 77] as index
             Button-${index}:
                css display is "block"
                css color contains "255, 255, 255"
                css font-size is "12px"

         #Table properties
          Table-*:
            css display is "table"
            css color contains "91, 103, 112"
            css font-size is "14px"