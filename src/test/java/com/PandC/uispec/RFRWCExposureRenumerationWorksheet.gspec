#Declaring objects with css and xpath locators
@objects
  Breadcrumbs                        css    .container-fluid .title-top .d-none
  Header                             css    #main-id .new-admin-header
  TabBreadcrumbs                     css    .container-fluid .title-top .breadcrumb-tab
  NamedInsured                       css    .container-fluid .name-insured
  TabHeader                          css    #USRevenueTab
  Label-*                            css    .container-fluid #myTabContent label
  Input-*                            css    .container-fluid #myTabContent input
  Select-*                           css    .container-fluid #myTabContent select
  TextArea-*                         css    .container-fluid #myTabContent textarea
  H3Text-*                           css    .container-fluid #myTabContent h3
  H2Text-*                           css    .container-fluid #myTabContent h2
  H1Text-*                           css    .container-fluid #myTabContent h1
  Span-*                            css    .container-fluid #myTabContent span
  Anchor-*                           css    .container-fluid #myTabContent a
  Button-*                           css    .container-fluid #myTabContent button
  TableHeader                        css    .container-fluid #tblUSCorporateRemuneration


#RFRWCExposureRenumerationWorksheet Page
= RFRWCExposureRenumerationWorksheet page =
  #This is for Desktop
   @on desktop

       #Breadcrumbs properties
        Breadcrumbs:
           css display is "block"
           css color contains "33, 37, 41"
           css font-size is "16px"

       #Header properties
        Header:
           css display is "block"
           css color contains "33, 37, 41"
           css font-size is "16px"

       #TabBreadcrumbs properties
        TabBreadcrumbs:
           css display is "block"
           css color contains "33, 37, 41"
           css font-size is "16px"

       #NamedInsured properties
        NamedInsured:
           css display is "flex"
           css color contains "33, 37, 41"
           css font-size is "16px"

       #TabHeader properties
        TabHeader:
           css display is "block"
           css color contains "73, 80, 87"
           css font-size is "14px"

       #Label properties
        @for [1] as index
           Label-${index}:
              css display is "block"
              css color contains "33, 37, 41"
              css font-size is "16px"

       #Label properties
        @for [2-7] as index
           Label-${index}:
              css display is "block"
              css color contains "31, 42, 68"
              css font-size is "14px"

       #Label properties
        @for [8-11] as index
           Label-${index}:
              css display is "inline-block"
              css color contains "255, 255, 255"
              css font-size is "14px"

       #Label properties
        @for [12-17] as index
           Label-${index}:
              css display is "inline-block"
              css color contains "91, 103, 112"
              css font-size is "14px"

       #Input properties
        @for [2-8, 11] as index
           Input-${index}:
              css display is "block"
              css color contains "91, 103, 112"
              css font-size is "14px"

       #Input properties
        @for [17-18, 20-21, 23-24, 26-27, 29-30, 32-33, ] as index
           Input-${index}:
              css display is "inline-block"
              css color contains "91, 103, 112"
              css font-size is "14px"

       #Button properties
        @for [1-8] as index
           Button-${index}:
              css display is "block"
              css color contains "255, 255, 255"
              css font-size is "12px"

       #TextArea properties
        @for [1-2] as index
           TextArea-${index}:
              css display is "block"
              css color contains "91, 103, 112"
              css font-size is "14px"

       #H1Text properties
        @for [1-2] as index
           H1Text-${index}:
              css display is "block"
              css color contains "31, 42, 68"
              css font-size is "18px"

       #H2Text properties
        @for [1-2] as index
           H2Text-${index}:
              css display is "block"
              css color contains "255, 255, 255"
              css font-size is "14px"

       #H3Text properties
        @for [1-2] as index
           H3Text-${index}:
              css display is "block"
              css color contains "91, 103, 112"
              css font-size is "14px"

       #Span properties
        @for [1] as index
           Span-${index}:
              css display is "block"
              css color contains "91, 103, 112"
              css font-size is "14px"

       #Span properties
        @for [2] as index
           Span-${index}:
              css display is "inline"
              css color contains "255, 255, 255"
              css font-size is "12px"

       #Span properties
        @for [3-6] as index
           Span-${index}:
              css display is "inline-block"
              css color contains "91, 103, 112"
              css font-size is "12px"

       #Span properties
        @for [7] as index
           Span-${index}:
              css display is "inline-block"
              css color contains "31, 42, 68"
              css font-size is "14px"

       #Span properties
        @for [8] as index
           Span-${index}:
              css display is "inline-block"
              css color contains "91, 103, 112"
              css font-size is "14px"

       #Anchor properties
        @for [1-6] as index
           Anchor-${index}:
              css display is "block"
              css color contains "255, 255, 255"
              css font-size is "12px"

       #Anchor properties
        @for [7-8] as index
           Anchor-${index}:
              css display is "block"
              css color contains "0, 123, 255"
              css font-size is "16px"

       #TableHeader properties
        TableHeader:
           css display is "table"
           css color contains "91, 103, 112"
           css font-size is "16px"

