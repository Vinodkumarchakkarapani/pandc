#Declaring objects with css and xpath locators
@objects
  Breadcrumbs                        css    .container-fluid .title-top .d-none
  Header                             css    #main-id .admin-header-main-wrapper
  TabBreadcrumbs                     css    .container-fluid .title-top .breadcrumb-tab
  NamedInsured                       css    .container-fluid .name-insured
  TabHeader                          css    #SupplementaryCommisionTab
  Label-*                            css    .container-fluid #myTabContent #wcSupplementaryCommision label
  Input-*                            css    .container-fluid #myTabContent #wcSupplementaryCommision input
  Select-*                           css    .container-fluid #myTabContent #wcSupplementaryCommision select
  TextArea-*                         css    .container-fluid #myTabContent #wcSupplementaryCommision textarea
  Span-*                             css    .container-fluid #myTabContent #wcSupplementaryCommision span
  Anchor-*                           css    .container-fluid #myTabContent #wcSupplementaryCommision a


#RFRWCExposureSupplementaryApplication Page
= RFRWCExposureSupplementaryApplication page =
  #This is for Desktop
   @on desktop

       #Breadcrumbs properties
        Breadcrumbs:
           css display is "block"
           css color contains "33, 37, 41"
           css font-size is "16px"

       #Header properties
        Header:
           css display is "block"
           css color contains "33, 37, 41"
           css font-size is "16px"

       #TabBreadcrumbs properties
        TabBreadcrumbs:
           css display is "block"
           css color contains "33, 37, 41"
           css font-size is "16px"

       #NamedInsured properties
        NamedInsured:
           css display is "flex"
           css color contains "33, 37, 41"
           css font-size is "16px"

       #TabHeader properties
        TabHeader:
           css display is "block"
           css color contains "73, 80, 87"
           css font-size is "14px"

       #Label properties
        @for [1] as index
           Label-${index}:
              css display is "block"
              css color contains "33, 37, 41"
              css font-size is "16px"

       #Label properties
        @for [2-8, 59-72] as index
           Label-${index}:
              css display is "block"
              css color contains "31, 42, 68"
              css font-size is "14px"

       #Label properties
        @for [9-10] as index
           Label-${index}:
              css display is "block"
              css color contains "33, 37, 41"
              css font-size is "14px"

       #Label properties
        @for [11-58] as index
           Label-${index}:
              css display is "block"
              css color contains "91, 103, 112"
              css font-size is "14px"

       #Input properties
        @for [4, 8-10,13, 16, 19, 22, 25, 28, 31, 34, 37, 40, 43, 46, 49, 52, 55, 58, 61, 64, 67, 70, 73-75, 78, 81, 84-88, 91-96] as index
           Input-${index}:
              css display is "block"
              css color contains "91, 103, 112"
              css font-size is "14px"

       #TextArea properties
        @for [1-3] as index
           TextArea-${index}:
              css display is "block"
              css color contains "91, 103, 112"
              css font-size is "14px"

       #Span properties
        @for [1, 3-80, 83-84] as index
           Span-${index}:
              css display is "block"
              css color contains "91, 103, 112"
              css font-size is "14px"

       #Span properties
        @for [2, 81-82, 85] as index
           Span-${index}:
              css display is "block"
              css color contains "33, 37, 41"
              css font-size is "16px"

       #Span properties
        @for [86] as index
           Span-${index}:
              css display is "inline"
              css color contains "255, 0, 0"
              css font-size is "18px"

       #Span properties
        @for [87] as index
           Span-${index}:
              css display is "inline-block"
              css color contains "31, 42, 68"
              css font-size is "14px"

       #Span properties
        @for [88] as index
           Span-${index}:
              css display is "inline-block"
              css color contains "91, 103, 112"
              css font-size is "14px"

       #Anchor properties
        @for [1-2] as index
           Anchor-${index}:
              css display is "block"
              css color contains "0, 123, 255"
              css font-size is "16px"



