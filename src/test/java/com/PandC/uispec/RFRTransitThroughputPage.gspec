#Declaring objects with css and xpath locators
@objects
  Breadcrumbs                        css    .breadcrumb-tab
  Header                             css    #main-id .new-admin-header
  TabBreadcrumbs                     css    #main-id .container-fluid .breadcrumb-tab
  InfoHeader                         css    #main-id .name-insured
  Label-*                            css    #main-id label
  Input-*                            css    #main-id input
  Select-*                           css    #main-id select
  TextArea-*                         css    #main-id textarea
  H4Text-*                           css    #main-id h4
  H2Text-*                           css    #main-id h2
  Alert-*                            css    #main-id .spnAlert
  Anchor-*                           css    #main-id a
  Button-*                           css    #main-id button
  TableTd-*                          css    #main-id #tblShipment td


#RFRCover Page
= RFRCover page =
  #This is for Desktop
   @on desktop

       #Breadcrumbs properties
        Breadcrumbs:
           css display is "block"
           css color contains "33, 37, 41"
           css font-size is "16px"

       #Header properties
        Header:
           css display is "block"
           css color contains "33, 37, 41"
           css font-size is "16px"

       #TabBreadcrumbs properties
        TabBreadcrumbs:
           css display is "block"
           css color contains "33, 37, 41"
           css font-size is "16px"

       #InfoHeader properties
        InfoHeader:
           css display is "flex"
           css color contains "33, 37, 41"
           css font-size is "16px"

       #Label properties
        @for [86] as index
           Label-${index}:
              css display is "block"
              css color contains "33, 37, 41"
              css font-size is "16px"

       #Label properties
        @for [87 - 88] as index
           Label-${index}:
              css display is "block"
              css color contains "31, 42, 68"
              css font-size is "14px"

       #Label properties
        @for [89 - 90] as index
           Label-${index}:
              css display is "block"
              css color contains "33, 37, 41"
              css font-size is "14px"

       #Label properties
        @for [91] as index
           Label-${index}:
              css display is "inline-block"
              css color contains "255, 255, 255"
              css font-size is "14px"

       #Label properties
        @for [92 - 107] as index
           Label-${index}:
              css display is "inline-block"
              css color contains "31, 42, 68"
              css font-size is "14px"

       #Input properties
        @for [216 - 271] as index
           Input-${index}:
              css display is "block"
              css color contains "91, 103, 112"
              css font-size is "14px"

       #Input properties
        @for [274 - 275, 277 - 278, 280 -281, 283 - 284, 286 - 287, 289 - 290, 292 - 293, 295 - 296, 298 - 299, 301 - 302, 304 - 305, 307 - 308, 310 - 311, 313 - 314, 316 - 317, 319 - 320] as index
           Input-${index}:
              css display is "inline-block"
              css color contains "91, 103, 112"
              css font-size is "14px"

       #Button properties
        @for [49-52] as index
           Button-${index}:
              css display is "block"
              css color contains "255, 255, 255"
              css font-size is "12px"

       #TextArea properties
        @for [8 - 10] as index
           TextArea-${index}:
              css display is "block"
              css color contains "91, 103, 112"
              css font-size is "14px"

       #TextArea properties
        @for [11] as index
           TextArea-${index}:
              css display is "inline-block"
              css color contains "91, 103, 112"
              css font-size is "14px"

       #H2Text properties
        @for [8] as index
           H2Text-${index}:
              css display is "block"
              css color contains "91, 103, 112"
              css font-size is "18px"

       #TableTd properties
        @for [1 - 36] as index
           TableTd-${index}:
              css display is "table-cell"
              css color contains "31, 42, 68"
              css font-size is "14px"