#Declaring objects with css and xpath locators
@objects
  Breadcrumbs                        css    .breadcrumb-tab
  Header                             css    #main-id .new-admin-header
  TabBreadcrumbs                     css    #main-id .container-fluid .breadcrumb-tab
  InfoHeader                         css    #main-id .name-insured
  Label-*                            css    #main-id label
  Input-*                            css    #main-id input
  Select-*                           css    #main-id select
  TextArea-*                         css    #main-id textarea
  Anchor-*                           css    #main-id a
  Button-*                           css    #main-id button
  DownloadTemplateButton-*           css    #main-id #excelDownloadAutoPolicy



#RFRCasualtyExposureAutoTab Page
= RFRCasualtyExposureAutoTab page =
  #This is for Desktop
     @on desktop

         #Breadcrumbs properties
          Breadcrumbs:
             css display is "block"
             css color contains "33, 37, 41"
             css font-size is "16px"

         #Header properties
          Header:
             css display is "block"
             css color contains "33, 37, 41"
             css font-size is "16px"

         #TabBreadcrumbs properties
          TabBreadcrumbs:
             css display is "block"
             css color contains "33, 37, 41"
             css font-size is "16px"

         #InfoHeader properties
          InfoHeader:
             css display is "flex"
             css color contains "33, 37, 41"
             css font-size is "16px"

         #Label properties
          Label-128:
             css display is "block"
             css color contains "33, 37, 41"
             css font-size is "16px"

         #Label properties
          @for [129, 130, 132] as index
             Label-${index}:
                css display is "inline-block"
                css color contains "255, 255, 255"
                css font-size is "14px"

         #Label properties
          @for [131, 133 - 140] as index
             Label-${index}:
                css display is "inline-block"
                css color contains "91, 103, 112"
                css font-size is "14px"

         #Label properties
          @for [141 - 156] as index
             Label-${index}:
                css display is "block"
                css color contains "91, 103, 112"
                css font-size is "14px"

         #Input properties
          @for [251, 254, 262 - 267, 270 - 271, 273 - 274, 276 - 277, 279 - 280, 282 - 283, 285 - 286, 288 - 289, 291 - 292, 301 - 302] as index
             Input-${index}:
                css display is "block"
                css color contains "91, 103, 112"
                css font-size is "14px"

         #Input properties
          @for [258 - 261] as index
             Input-${index}:
                css display is "inline-block"
                css color contains "91, 103, 112"
                css font-size is "14px"

         #TextArea properties
          @for [5 - 6] as index
             TextArea-${index}:
                css display is "block"
                css color contains "91, 103, 112"
                css font-size is "14px"

         #Anchor properties
          @for [1, 3] as index
             Anchor-${index}:
                css display is "block"
                css color contains "0, 123, 255"
                css font-size is "16px"

         #Anchor properties
          Anchor-6:
            css display is "block"
            css color contains "120, 190, 32"
            css font-size is "14px"

         #Anchor properties
          Anchor-7:
            css display is "block"
            css color contains "31, 42, 68"
            css font-size is "14px"

         #Anchor properties
          @for [15 - 16] as index
             Anchor-${index}:
               css display is "block"
               css color contains "0, 115, 150"
               css font-size is "14px"

         #Anchor properties
          @for [17 - 23] as index
             Anchor-${index}:
                css display is "block"
                css color contains "255, 255, 255"
                css font-size is "16px"

         #Anchor properties
          @for [24 - 26, 28 - 30] as index
             Anchor-${index}:
                css display is "block"
                css color contains "255, 255, 255"
                css font-size is "14px"

         #Anchor properties
          @for [38 - 40, 42 - 43] as index
              Anchor-${index}:
                css display is "block"
                css color contains "255, 255, 255"
                css font-size is "12px"

         #Anchor properties
          Anchor-27:
             css display is "block"
             css color contains "73, 80, 87"
             css font-size is "14px"

         #Anchor properties
          Anchor-41:
             css display is "block"
             css color contains "0, 123, 255"
             css font-size is "14px"


         #Anchor properties
          Anchor-44:
            css display is "block"
            css color contains "0, 123, 255"
            css font-size is "16px"

         #Button properties
          @for [35 - 42] as index
             Button-${index}:
                css display is "block"
                css color contains "255, 255, 255"
                css font-size is "12px"

         #DownloadTemplateButton properties
          DownloadTemplateButton-*:
             css display is "block"
             css color contains "255, 255, 255"
             css font-size is "12px"
             css background-color contains "52, 101, 127"
