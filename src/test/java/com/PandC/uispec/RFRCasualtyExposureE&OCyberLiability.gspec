#Declaring objects with css and xpath locators
@objects
  Breadcrumbs                        css    .breadcrumb-tab
  Header                             css    #main-id .new-admin-header
  TabBreadcrumbs                     css    #main-id .container-fluid .breadcrumb-tab
  InfoHeader                         css    #main-id .name-insured
  Label-*                            css    #main-id label
  Input-*                            css    #main-id input
  Select-*                           css    #main-id select
  TextArea-*                         css    #main-id textarea
  Anchor-*                           css    #main-id a
  Button-*                           css    #main-id button
  GridContent                        css    #main-id #tblEAndOCyberLiability



#RFRCasualtyExposureE&OCyberLiability Page
= RFRCasualtyExposureE&OCyberLiability page =
  #This is for Desktop
     @on desktop

         #Breadcrumbs properties
          Breadcrumbs:
             css display is "block"
             css color contains "33, 37, 41"
             css font-size is "16px"

         #Header properties
          Header:
             css display is "block"
             css color contains "33, 37, 41"
             css font-size is "16px"

         #TabBreadcrumbs properties
          TabBreadcrumbs:
             css display is "block"
             css color contains "33, 37, 41"
             css font-size is "16px"

         #InfoHeader properties
          InfoHeader:
             css display is "flex"
             css color contains "33, 37, 41"
             css font-size is "16px"

         #Label properties
          Label-163:
             css display is "block"
             css color contains "33, 37, 41"
             css font-size is "16px"

         #Label properties
          @for [164] as index
             Label-${index}:
                css display is "inline-block"
                css color contains "255, 255, 255"
                css font-size is "14px"

         #Label properties
          @for [165 - 181] as index
             Label-${index}:
                css display is "inline-block"
                css color contains "91, 103, 112"
                css font-size is "14px"

         #Label properties
          Label-182:
             css display is "inline-block"
             css color contains "255, 255, 255"
             css font-size is "14px"

         #Input properties
          @for [314 - 315, 319 - 318, 320 - 321, 323 - 324, 326- 327, 329 - 330, 332 - 333, 335 - 336, 338 - 339, 341 - 342, 344 - 345, 347 - 348, 350 - 351, 353 - 354, 356 - 357, 359 - 360, 362 - 363] as index
             Input-${index}:
                css display is "inline-block"
                css color contains "91, 103, 112"
                css font-size is "14px"

         #TextArea properties
          TextArea-8:
             css display is "block"
             css color contains "91, 103, 112"
             css font-size is "14px"

         #Anchor properties
          @for [1, 3] as index
             Anchor-${index}:
                css display is "block"
                css color contains "0, 123, 255"
                css font-size is "16px"

         #Anchor properties
          Anchor-6:
            css display is "block"
            css color contains "120, 190, 32"
            css font-size is "14px"

         #Anchor properties
          Anchor-7:
            css display is "block"
            css color contains "31, 42, 68"
            css font-size is "14px"

         #Anchor properties
          @for [15 - 16] as index
             Anchor-${index}:
               css display is "block"
               css color contains "0, 115, 150"
               css font-size is "14px"

         #Anchor properties
          @for [17 - 23] as index
             Anchor-${index}:
                css display is "block"
                css color contains "255, 255, 255"
                css font-size is "16px"

         #Anchor properties
          @for [24 - 28, 30] as index
             Anchor-${index}:
                css display is "block"
                css color contains "255, 255, 255"
                css font-size is "14px"

         #Anchor properties
          Anchor-29:
            css display is "block"
            css color contains "73, 80, 87"
            css font-size is "14px"

         #Anchor properties
          @for [45 - 46] as index
              Anchor-${index}:
                css display is "block"
                css color contains "255, 255, 255"
                css font-size is "12px"

         #Button properties
          @for [58 - 59, 84 - 85] as index
              Button-${index}:
                css display is "block"
                css color contains "255, 255, 255"
                css font-size is "12px"

         #GridContent properties
          GridContent:
            css display is "table"
            css color contains "91, 103, 112"
            css font-size is "16px"